        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Closest Planet &amp; void
if instance_number(oVoid)&gt;0 void=instance_nearest(x,y,oVoid)
else void=id

if instance_number(oPlanet)&gt;0 closestplanet=instance_nearest(x,y,oPlanet)
else closestplanet=id

if room=rPlanet&amp;&amp;jump{
    global.preroom=room
    room_goto(rJump)
}

if room=rSpace&amp;&amp;oVoid!=id&amp;&amp;distance_to_object(void)&lt;=128&amp;&amp;jump{
    global.preroom=room
    room_goto(rWarp)
}

if closestplanet!=id&amp;&amp;room=rSpace{
    global.planetcolor=closestplanet.basecolor
    global.planetsize=closestplanet.size
    global.planetx=closestplanet.x
    global.planety=closestplanet.y
    global.preroom=room
    xdif=x-closestplanet.x
    ydif=y-closestplanet.y
    if distance_to_object(closestplanet)&lt;=closestplanet.size&amp;&amp;jump&amp;&amp;instance_number(oVoid)=0{
        global.planetoil=closestplanet.oil 
        room_goto(rJump)
    }
}

if distance_to_object(closestplanet)&lt;256&amp;&amp;closestplanet!=id closetoplanet=true
else closetoplanet=false

</string>
