        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement
if keyboard_check_pressed(vk_escape) game_end()

scrControls()
audio_listener_orientation(0,0,1000,0,-1,0)
audio_listener_position(x, y, 0)

shield.image_angle=image_angle-(turn*5)
shield.x=x
shield.y=y

if shieldswitch{
    shield.side*=-1
    shield.charge=0
}

if shoot shield.charge-=0.3

turning=left-right
if boost{
    up*=4
    turning*=3
}
else up*=2

if boostlaunch up=60

if(x&lt;0||x&gt;room_width||y&lt;0||y&gt;room_width){
    turning=sign(angle_difference(point_direction(x,y,room_width/2,room_height/2),image_angle))
    up = 6
}

if place_meeting(x,y,oAsteroid){
    //direction=point_direction(other.x,other.y,x,y)
    if disabled=0 with instance_place(x,y,oAsteroid) motion_add(point_direction(other.x,other.y,x,y),other.speed/size)
    //if speed&gt;1 with instance_place(x,y,oAsteroid) instance_destroy()
    speed+=1
    speed*=-1
    disabled+=30
}

if disabled!=0{
    up=0
    turning=0
}

fuel=clamp(fuel-up/240,0,500)

turn = ((turn*29)+(turning*3))/30
image_angle+=turn
if up&gt;0 direction = image_angle

disabled=clamp(disabled-1,0,180)

speed = ((speed*59)+up)/60

if speed&gt;0.1 with instance_create(x,y,oFlame) image_blend=make_colour_hsv(75,255,255)
</string>
