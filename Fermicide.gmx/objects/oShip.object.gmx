if spawnvoidtime=true&amp;&amp;room=rSpace&amp;&amp;!instance_exists(oVoid) instance_create(random_range(200,room_width-200),random_range(200,room_height-200),oVoid)

if voidspawn&gt;1800{
    spawnvoidtime=false
    if room=rSpace&amp;&amp;instance_exists(oVoid){
        instance_create(-8,-8,oLargeEnemy)
        instance_create(-8,room_height+8,oLargeEnemy)
        instance_create(room_width++,-8,oLargeEnemy)
        instance_create(room_width+8,room_height+8,oLargeEnemy)
    }
