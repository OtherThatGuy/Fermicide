draw_self()

draw_set_circle_precision(64)

if distance_to_object(closestenemy)&lt;256&amp;&amp;closestenemy!=id{
    draw_set_colour(c_green)
    draw_set_alpha(1.1-(distance_to_object(closestenemy)/256))
    draw_line(x,y,closestenemy.x,closestenemy.y)
    draw_circle(closestenemy.x,closestenemy.y,12+distance_to_object(closestenemy)/6,true)
    draw_set_colour(c_white)
    draw_set_alpha(1)
}

if instance_number(oVoid)&gt;0&amp;&amp;void!=id{
    draw_set_colour(c_red)
    draw_line(x,y,void.x,void.y)
    draw_set_alpha(1-(distance_to_object(void)/128))
    draw_circle(void.x,void.y,96,true)
    draw_circle(void.x,void.y,97,true)
    draw_set_colour(c_white)
    draw_set_alpha(1)
}

if distance_to_object(closestplanet)&lt;256&amp;&amp;closestplanet!=id{
    draw_set_colour(c_white)
    draw_set_alpha(1-(distance_to_object(closestplanet)/256))
    draw_line(x,y,closestplanet.x,closestplanet.y)
    draw_circle(closestplanet.x,closestplanet.y,closestplanet.size,true)
    draw_circle(closestplanet.x,closestplanet.y,closestplanet.size+1,true)
    draw_set_colour(c_white)
    draw_set_alpha(1)
}

if instance_number(oOil)&gt;0&amp;&amp;distance_to_object(closestoil)&gt;5&amp;&amp;closestoil!=id{
    draw_set_colour(c_black)
    draw_circle(x,y,oilradar,true)
    draw_circle(x,y,oilradar+1,true)
    draw_set_colour(c_white)
    draw_set_alpha(1)
}


if disabled!=0 draw_circle_colour(x,y,disabled/10,c_yellow,c_yellow,true)
