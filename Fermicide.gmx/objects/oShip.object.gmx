            <string>if room=rPlanet{
    speed=5
    oilradar=0
    background_colour=global.planetcolor
    x=room_width/2
    y=room_height/2
}
else if room=rJump||room=rWarp{
    disabled=60
    if room=rWarp disabled=120
    turn=0
    speed=0
    direction=image_angle
    if global.preroom=rPlanet||room=rWarp{
        x=room_width/2
        y=room_height/2
    }
    else if global.preroom=rSpace{
        x=room_width/2+xdif
        y=room_height/2+ydif
        if global.jumps&gt;6 &amp;&amp;random(12)&gt;11 global.winningplanet=true
    }
}
else if room=rSpace{
    if global.preroom=rPlanet{
        x=global.planetx
        y=global.planety
        closestplanet.oil=global.planetoil
    }
    if global.preroom=rSpace{
        with oSpaceGen instance_destroy()
        instance_create(room_width/2,room_height/2,oSpaceGen)
        spawnvoidtime=false
        spawnvoid=1
        x=room_width/2
        y=room_height/2
        showspacename=180
        global.jumps++
    }
    speed=5
}

scrSpaceName()
