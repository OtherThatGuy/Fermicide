if instance_number(oEnemy)&gt;0 closestenemy=instance_nearest(x,y,oEnemy)
else closestenemy=id

nextshot=(nextshot+1) mod 16

if round(nextshot)=1&amp;&amp;shoot{
    if distance_to_object(closestenemy)&lt;256&amp;&amp;closestenemy!=id with instance_create(x,y,oLaser){
        image_angle=point_direction(x,y,other.closestenemy.x,other.closestenemy.y)
    }
    else with instance_create(x,y,oLaser){
        image_angle=other.image_angle
    }
