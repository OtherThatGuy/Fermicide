draw_healthbar(2,710,144,720,fuel/5,c_black,c_dkgray,c_ltgray,0,true,true)

showspacename=clamp(showspacename-1,0,180)

if global.winningplanet=true text="Life detected"
else if spawnvoidtime=true{
    if room=rSpace text="Warp Void detected, proceed immediately"
    else if room=rPlanet text="Warp Void detected, leave the planet immediately"
}
else if showspacename&gt;0 text=global.spacename
else if instance_exists(oOil)&amp;&amp;!place_meeting(x,y,oOil) text="Oil detected, pinging for deposits"
else if place_meeting(x,y,oOil)&amp;&amp;speed&gt;0.6&amp;&amp;speed&lt;3.5 text="Oil found, slow down to withdraw"
else if place_meeting(x,y,oOil)&amp;&amp;speed&lt;0.6 text="Withdrawing oil"
else if room=rJump text="Initiating small scale jump"
else if room=rWarp text="Initiating large scale jump"
else text=""

if text!=""{
    draw_set_alpha(0.2)
    draw_rectangle_color(0,0,1280,58,c_green,c_green,c_green,c_green,false)
    draw_text_colour(0,10,text,c_red,c_red,c_red,c_red,0.1)
    draw_text_colour(4,10,text,c_blue,c_blue,c_blue,c_blue,0.1)
    draw_text(2,10,text)
    draw_set_alpha(1)
}

